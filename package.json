{
  "name": "plsinstall",
  "displayName": "Python PlsInstall",
  "description": "Quick and easy pip installer",
  "version": "0.0.1",
  "publisher": "Mason Lapine",
  "engines": {
    "vscode": "^1.62.0"
  },
  "activationEvents": [
    "onCommand:extension.installPythonPackage",
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.installPythonPackage",
        "title": "Install Python Package"
      }
    ],
    "languages": [
      {
        "id": "python"
      }
    ],
    "codeActions": [
      {
        "language": "python",
        "kind": "quickfix",
        "title": "Install Python Package"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/vscode": "^1.62.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.4.1"
  }
}